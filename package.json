{
    "name": "roblox-fastflags",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "package": "electron-builder"
    },
    "dependencies": {
      "electron": "^22.0.0"
    },
    "devDependencies": {
      "electron-builder": "^23.0.0"
    },
    "build": {
      "appId": "com.loaf.rblxflags",
      "productName": "Roblox fastflags",
      "directories": {
        "output": "dist"
      },
      "files": [
        "fastflags/**/*",
        "node_modules/**/*",
        "package.json",
        "main.js"
      ],
      "mac": {
        "target": "dmg"
      },
      "win": {
        "target": "nsis"
      },
      "linux": {
        "target": "AppImage"
      }
    }
  }
  