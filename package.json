{
  "name": "roblox-fflags",
  "version": "1.0.0",
  "description": "collection of roblox fflagsr",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.loaf.robloxfflags",
    "productName": "robloxfflags",
    "files": [
      "**/*",
      "!**/node_modules/*/{test,__tests__,example}/**"
    ],
    "extraResources": [
      "assets",
      "fastflags",
      "help",
      "logs",
      "resources"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^29.0.0",
    "electron-builder": "^24.6.0"
  }
}
